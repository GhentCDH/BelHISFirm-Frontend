{
    "id": "stocks",
    "endpoint": {
        "url": "http://localhost:8080/sparql",
        "useAuth": false,
        "prefixesFile": "SparqlQueriesPrefixes.js",
        "defaultSparql": true
    },
    "sparqlQueriesFile": "ScobStocks.js",
    "baseURI": "http://belhisfirm.be/resource",
    "URITemplate": "<BASE_URI>/stock/<LOCAL_ID>",
    "resultClasses": {
        "stocks": {
            "instanceConfig": {
                "propertiesQueryBlock": "stockPropertiesInstancePage",
                "instancePageResultClasses": {
                    "instancePageTable": {
                        "tabID": 0,
                        "component": "InstancePageTable",
                        "tabPath": "table",
                        "tabIcon": "CalendarViewDay"
                    }
                }
            }
        }
    },
    "properties": [
        {
            "id": "uri",
            "valueType": "object",
            "minWidth": 250,
            "makeLink": true,
            "externalLink": false
        },
        {
            "id": "sharetype",
            "valueType": "object",
            "minWidth": 250
        },
        {
            "id": "stockExchange",
            "valueType": "object",
            "minWidth": 250
        },
        {
            "id": "corporation",
            "valueType": "object",
            "minWidth": 250,
            "makeLink": true,
            "externalLink": false
        }
    ]
}