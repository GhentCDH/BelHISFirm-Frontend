{
    "id": "stocks",
    "endpoint": {
        "url": "http://localhost:8080/sparql",
        "useAuth": false,
        "prefixesFile": "SparqlQueriesPrefixes.js",
        "defaultSparql": true
    },
    "sparqlQueriesFile": "ScobStocks.js",
    "baseURI": "http://belhisfirm.be/resource",
    "URITemplate": "<BASE_URI>/stock/<LOCAL_ID>",
    "resultClasses": {
        "stocks": {
            "instanceConfig": {
                "propertiesQueryBlock": "stockPropertiesInstancePage",
                "instancePageResultClasses": {
                    "instancePageTable": {
                        "tabID": 0,
                        "component": "InstancePageTable",
                        "tabPath": "table",
                        "tabIcon": "CalendarViewDay"
                    },
                    "openCloseValuesChart": {
                        "tabID": 1,
                        "component": "ApexCharts",
                        "tabPath": "openClose",
                        "tabIcon": "ShowChart",
                        "pageType": "instancePage",
                        "sparqlQuery": "stocksGraphOpenClose",
                        "resultMapper": "mapTimelineChart",
                        "resultMapperConfig": {
                            "fillEmptyValues": false,
                            "dateCategory": true,
                            "customizedCategoryLabels": true
                        },
                        "createChartData": "createMultipleLineChartData",
                        "title": "Recorded stock prices",
                        "xaxisTitle": "day",
                        "xaxisType": "datetime",
                        "xaxisTickAmount": 30,
                        "yaxisTitle": "price",
                        "seriesTitle": "price",
                        "stroke": {
                            "curve": "straight",
                            "width": 2
                        },
                        "tooltip": {
                            "x": {
                                "format": "dd MMM yyyy"
                            }
                        },
                        "fill": {
                            "type": "gradient",
                            "gradient": {
                                "shadeIntensity": 1,
                                "inverseColors": false,
                                "opacityFrom": 0.6,
                                "opacityTo": 0.05,
                                "stops": [
                                    20,
                                    60,
                                    100,
                                    100
                                ]
                            }
                        }
                    }
                }
            }
        }
    },
    "properties": [
        {
            "id": "uri",
            "valueType": "object",
            "minWidth": 250,
            "makeLink": true,
            "externalLink": false
        },
        {
            "id": "sharetype",
            "valueType": "object",
            "minWidth": 250
        },
        {
            "id": "stockExchange",
            "valueType": "object",
            "minWidth": 250
        },
        {
            "id": "corporation",
            "valueType": "object",
            "minWidth": 250,
            "makeLink": true,
            "externalLink": false
        },
        {
            "id": "name",
            "valueType": "object",
            "minwidth": 250,
            "sortValues": true
        }
    ]
}