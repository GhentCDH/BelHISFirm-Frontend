@prefix owl:  <http://www.w3.org/2002/07/owl#> .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dct:  <http://purl.org/dc/terms/> .

<http://belhisfirm.be/ontology>
    rdf:type     owl:Ontology ;
    owl:imports  <ontology/general.ttl> ;
    owl:imports  <ontology/corporations.ttl> ;
    owl:imports  <ontology/stocks.ttl> ;
    rdfs:label   "BellHisFirm Ontology"@en ;
    rdfs:comment "Ontology for Belgian Historical Firm Data (SCOB)"@en ;
    dct:creator  "BellHisFirm Project" ;
    dct:description
                 "An ontology for representing Belgian historical corporation data, persons, and their relationships"@en .
